# Ignore files generated by Docker
.dockerignore
Dockerfile.*
docker-compose.*
docker-compose-*.yml

# Ignore Docker-specific files and directories
.docker
.idea

# Ignore Docker build context files
*.tar.gz
*.tar
*.zip

# Ignore Docker container logs
*.log
*.log.*

# Ignore Docker image files
*.img
*.iso

# Ignore local development environment files
.env
.env.*

# Ignore editor-specific files
.vscode/
*.sublime-project
*.sublime-workspace

# Ignore system-specific files
.DS_Store
Thumbs.db
